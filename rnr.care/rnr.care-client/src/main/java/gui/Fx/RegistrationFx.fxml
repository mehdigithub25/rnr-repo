<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="473.0" prefWidth="600.0" stylesheets="@style.css"
	xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="gui.controller.RegistrationController">
	<children>
		<Button fx:id="btn_register" layoutX="394.0" layoutY="203.0"
			mnemonicParsing="false" onAction="#RegisterAction" text="register" />
		<TextField fx:id="lastName" layoutX="76.0" layoutY="83.0"
			promptText="Last name" />
		<TextField fx:id="firstName" layoutX="76.0" layoutY="31.0"
			promptText="First name" />
		<TextField fx:id="userName" layoutX="76.0" layoutY="132.0"
			promptText="user name" />
		<PasswordField fx:id="password" layoutX="76.0" layoutY="190.0"
			promptText="password" />
		<TextField fx:id="email" layoutX="76.0" layoutY="246.0"
			promptText="email" />
		<TextField fx:id="address" layoutX="76.0" layoutY="299.0"
			promptText="address" />
		<TextField fx:id="numPhone" layoutX="76.0" layoutY="352.0"
			promptText="phone number" />
		<ChoiceBox fx:id="choiceId" layoutX="333.0" layoutY="71.0"
			onMouseClicked="#refrechAction" prefWidth="150.0" />

	</children>
</AnchorPane>
